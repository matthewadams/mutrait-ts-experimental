{
  "name": "@northscaler/mutrait-ts",
  "version": "0.1.0-pre.0",
  "description": "Traits for TypeScript",
  "scripts": {
    "prebuild": "npm install",
    "build": "npm test",
    "transpile": "tsc",
    "pretest": "npm run transpile",
    "test": "siesta ./tests",
    "posttest": "npm run lint",
    "lint": "eslint '{src,tests}/**/*.{ts,json}'",
    "lint:fix": "eslint --fix '{src,tests}/**/*.{ts,json}'",
    "format": "prettier --write '{src,tests}/**/*.{ts,yaml,yml,json,md}'"
  },
  "license": "MIT",
  "dependencies": {
    "esm": "3.2.25",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.0",
    "@types/node": "12.7.2",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "core-js": "2.6.5",
    "deoptigate": "0.5.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "prettier": "2.2.1",
    "regenerator-runtime": "0.13.1",
    "siesta-lite": "5.5.0",
    "typedoc": "git://github.com/bryntum/typedoc.git#custom",
    "typedoc-default-themes": "git://github.com/bryntum/typedoc-default-themes.git",
    "typescript": "3.8.3"
  },
  "prettier": {
    "semi": false,
    "printWidth": 132,
    "tabWidth": 2,
    "useTabs": false,
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "none"
  },
  "volta": {
    "node": "14.15.4"
  }
}
